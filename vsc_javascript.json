    },
    "Print to console bd": {
        "prefix": "bd",
        "body": ["border: 0.1rem solid green;"],
        "description": "Border"
    },
    "Print to console lk": {
        "prefix": "lk",
        "body": ["console.log('%c %s', 'background: #0d7200; color: white', {$1})"],
        "description": "Border"
    },
